#include<iostream>
using namespace std;
int main()
{
	int a[10],n,i,j,number;
	cout<<"enter no of elements";
	cin>>n;
	cout<<"enter alements";
	for(i=0;i<n;i++)
	{
		cin>>a[i];
	}
	cout<<"enter the element to be searched";
	cin>>number;
	
	
	for(i=0;i<n;i++)
	{
		if(number==a[i])
	{
		cout<<"the number is present at"<<i+1;	
		
		}
	}
}
